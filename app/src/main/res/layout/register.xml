<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/basic_background"
    tools:context=".MainActivity">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/constraintLayout"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@drawable/basic_background"
        tools:context=".MainActivity"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="0dp">

        <TextView
            android:id="@+id/textView4"
            android:layout_width="109dp"
            android:layout_height="19dp"
            android:width="73dp"
            android:fontFamily="@font/roboto"
            android:text="Enter username:"
            android:textColor="#FFFFFF"
            android:textSize="15sp"
            tools:ignore="MissingConstraints"
            tools:layout_editor_absoluteX="51dp"
            tools:layout_editor_absoluteY="443dp" />

        <TextView
            android:id="@+id/textView6"
            android:layout_width="109dp"
            android:layout_height="19dp"
            android:width="73dp"
            android:fontFamily="@font/roboto"
            android:text="Enter full name:"
            android:textColor="#FFFFFF"
            android:textSize="15sp"
            tools:ignore="MissingConstraints"
            tools:layout_editor_absoluteX="51dp"
            tools:layout_editor_absoluteY="356dp" />

        <TextView
            android:id="@+id/textView7"
            android:layout_width="284dp"
            android:layout_height="16dp"
            android:fontFamily="@font/roboto"
            android:text="Already have an account? Log in"
            android:textAlignment="center"
            android:textColor="#FFFFFF"
            android:textSize="13sp"
            android:textStyle="normal"
            tools:layout_editor_absoluteX="63dp"
            tools:layout_editor_absoluteY="677dp" />

        <ImageView
            android:id="@+id/imageView"
            android:layout_width="238dp"
            android:layout_height="79dp"
            app:srcCompat="@drawable/_12"
            tools:layout_editor_absoluteX="86dp"
            tools:layout_editor_absoluteY="84dp" />

        <ImageView
            android:id="@+id/imageView3"
            android:layout_width="45dp"
            android:layout_height="24dp"
            app:srcCompat="@drawable/greece"
            tools:layout_editor_absoluteX="302dp"
            tools:layout_editor_absoluteY="24dp" />

        <ImageView
            android:id="@+id/imageView3"
            android:layout_width="45dp"
            android:layout_height="24dp"
            app:srcCompat="@drawable/united_kingdom__1_"
            tools:layout_editor_absoluteX="347dp"
            tools:layout_editor_absoluteY="24dp" />

        <Button
            android:id="@+id/button"
            android:layout_width="298dp"
            android:layout_height="44dp"
            android:width="298dp"
            android:backgroundTint="#7D5260"
            android:text="Register now"
            android:textColor="#FFFFFF"
            android:textSize="15sp"
            tools:ignore="MissingConstraints"
            tools:layout_editor_absoluteX="56dp"
            tools:layout_editor_absoluteY="596dp" />

        <TextView
            android:id="@+id/textView"
            android:layout_width="156dp"
            android:layout_height="40dp"
            android:fontFamily="@font/roboto_bold"
            android:text="Register"
            android:textAlignment="center"
            android:textColor="#FFFFFF"
            android:textSize="32sp"
            android:textStyle="bold"
            tools:layout_editor_absoluteX="127dp"
            tools:layout_editor_absoluteY="269dp" />

        <EditText
            android:id="@+id/editTextTextPersonName5"
            android:layout_width="298dp"
            android:layout_height="27dp"
            android:width="298dp"
            android:duplicateParentState="false"
            android:editable="true"
            android:ems="10"
            android:fontFamily="@font/roboto"
            android:inputType="textPersonName|text"
            android:singleLine="true"
            android:textAlignment="viewStart"
            android:textColor="#FFFFFF"
            android:textSize="14sp"
            tools:layout_editor_absoluteX="51dp"
            tools:layout_editor_absoluteY="375dp" />

        <EditText
            android:id="@+id/editTextTextPersonName"
            android:layout_width="298dp"
            android:layout_height="27dp"
            android:width="298dp"
            android:duplicateParentState="false"
            android:editable="true"
            android:ems="10"
            android:fontFamily="@font/roboto"
            android:inputType="textPersonName|text"
            android:singleLine="true"
            android:textAlignment="viewStart"
            android:textColor="#FFFFFF"
            android:textSize="14sp"
            tools:layout_editor_absoluteX="51dp"
            tools:layout_editor_absoluteY="462dp" />

        <EditText
            android:id="@+id/editTextTextPersonName2"
            android:layout_width="298dp"
            android:layout_height="27dp"
            android:width="298dp"
            android:duplicateParentState="false"
            android:editable="true"
            android:ems="10"
            android:fontFamily="@font/roboto"
            android:inputType="textPersonName"
            android:singleLine="true"
            android:textAlignment="viewStart"
            android:textColor="#FFFFFF"
            android:textSize="14sp"
            tools:layout_editor_absoluteX="51dp"
            tools:layout_editor_absoluteY="539dp" />

        <TextView
            android:id="@+id/textView8"
            android:layout_width="109dp"
            android:layout_height="19dp"
            android:width="73dp"
            android:fontFamily="@font/roboto"
            android:text="Enter password:"
            android:textColor="#FFFFFF"
            android:textSize="15sp"
            tools:ignore="MissingConstraints"
            tools:layout_editor_absoluteX="51dp"
            tools:layout_editor_absoluteY="520dp" />

    </androidx.constraintlayout.widget.ConstraintLayout>

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="238dp"
        android:layout_height="79dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/_12"
        tools:layout_editor_absoluteY="84dp" />

</androidx.constraintlayout.widget.ConstraintLayout>